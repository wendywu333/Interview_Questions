package LeetCode;

/**
 * @author Wenzhe
 * @date 6/25/2013
 * 
 */
public class Median_of_Two_Sorted_Arrays {

	public double findMedianSortedArrays(int A[], int B[]) {
		// O(log(m+n))

		return 0.00;

	}

	private int GetMedian(int a[], int n, int b[], int m, int k) {
		if (n <= 0)
			return b[k - 1];
		if (m <= 0)
			return a[k - 1];
		if (k <= 1)
			return min(a[0], b[0]);

		return 0;
	}

	private int min(int i, int j) {
		// TODO Auto-generated method stub
		if (i < j)
			return i;
		else
			return j;
	}
}
