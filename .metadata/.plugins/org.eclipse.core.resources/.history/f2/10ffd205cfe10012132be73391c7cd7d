package LeetCode;

/**
 * @author Wenzhe
 * @date Jun 27, 2013
 * 
 */
public class First_Missing_Positive {

	public int firstMissingPositive(int[] A) {
		// Start typing your Java solution below
		// DO NOT write main() function

		int n = A.length;
		int temp;
		for (int i = 0; i < n; i++) {
			if (A[i] != i + 1) {

				// A[0] = 8 should be 1, put 8 to A[7];
				temp = A[A[i] - 1];
				A[A[i] - 1] = A[i];
				A[i] = temp;

			}

		}

		return 0;

	}

}
