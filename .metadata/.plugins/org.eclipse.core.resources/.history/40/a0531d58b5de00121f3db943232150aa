		ArrayList<String> comb = new ArrayList<String>();
	
		String[] dMap = buildDMap();

		for (int i = 0; i < digits.length(); i++) {
			ArrayList<String>
		}

		return comb;

	}

	private String[] buildDMap() {
		// TODO Auto-generated method stub
		String[] res = new String[8];
		for(int i = 0;i<8;i++){
			int count;
			if(i == 5 || i==7) count = 4;
			else count = 3;
		}
		return res;
	}

}
