package LeetCode;

/**
 * @author Wenzhe
 * @date 4/4/2013
 * 
 */
public class Sum_Root_to_Leaf_Numbers {

	public int sumNumbers(TreeNode root) {

		if (root == null)
			return 0;

		int level = 0;
		int result = recursiveSum(root, level);

		return 0;

	}

	private int recursiveSum(TreeNode root, int level) {
		// TODO Auto-generated method stub
		int result = root.val * (int)Math.pow(10, level) + recursiveSum(root.left,level)
		return result;
	}}
