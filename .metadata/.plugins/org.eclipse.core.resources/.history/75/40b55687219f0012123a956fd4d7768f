 */
package LeetCode;

/**
 * @author Wenzhe
 * @date 4/6/2013
 * 
 */
public class Unique_Paths {

	public int uniquePaths(int m, int n) {

		if (m == 1 || n == 1) {
			return 1;
		}
		int[][] table = new int[m][n];
		table[m][n] = 0;
		table[m - 1][n] = 1;
		table[m][n - 1] = 1;
		table[m - 1][n - 1] = 2;
		for (int i = m - 2; i > 0; i--) {
			for (int j = n - 2; j > 0; j--) {
				table[i][j] = 2 * table[i + 1][j + 1];
			}
		}

		return 0;

	}

}
