package LeetCode;

/**
 * @author Wenzhe
 * @date 6/26/2013
 * 
 */
public class Interleaving_String {

	public boolean isInterleave(String s1, String s2, String s3) {
		// Start typing your Java solution below
		// DO NOT write main() function
		if (s1.length() + s2.length() != s3.length())
			return false;

		return false;

	}

}
