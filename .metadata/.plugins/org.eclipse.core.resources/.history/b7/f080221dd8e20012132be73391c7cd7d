 */
public class Count_and_Say {
	
	
	
	public String countAndSay(int n) {
		// Start typing your Java solution below
		// DO NOT write main() function
		String res = "";
		if(n == 0) return res;
		res = "1";
		while(n > 1){
			n--;
			StringBuilder sb = new StringBuilder();
		}
		

		return res;

	}


}
